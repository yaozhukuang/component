<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zw.yzk.component">

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/component_res_ic_launcher"
        android:label="@string/component_res_app_name"
        android:roundIcon="@mipmap/component_res_ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/component_res_AppTheme">

        <activity
            android:name=".MainActivity"
            android:launchMode="singleTask"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>