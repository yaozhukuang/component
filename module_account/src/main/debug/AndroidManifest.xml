<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zw.yzk.module.account">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/component_res_ic_launcher"
        android:label="@string/module_account_app_name"
        android:roundIcon="@mipmap/component_res_ic_launcher_round"
        android:supportsRtl="true"
        android:name="ModuleAccountApplication"
        android:theme="@style/component_res_AppTheme">
        <activity android:name="com.zw.yzk.module.account.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".ui.activity.LoginActivity"
            android:exported="false"
            android:screenOrientation="portrait" />

        <activity
            android:name=".ui.activity.ArticleActivity"
            android:exported="false"
            android:screenOrientation="portrait" />

        <activity
            android:name=".ui.activity.NavigationActivity"
            android:exported="false"
            android:screenOrientation="portrait" />

        <activity
            android:name=".ui.activity.SettingsActivity"
            android:exported="false"
            android:screenOrientation="portrait" />

        <activity
            android:name=".ui.activity.WebsiteActivity"
            android:exported="false"
            android:screenOrientation="portrait" />

        <activity
            android:name=".ui.activity.RegisterActivity"
            android:exported="false"
            android:screenOrientation="portrait" />

        <activity
            android:name=".ui.activity.DownloadActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
    </application>

</manifest>