<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zw.yzk.module.article">

    <application
        android:name="com.zw.yzk.component.common.base.BaseApplication"
        android:allowBackup="true"
        android:icon="@mipmap/component_res_ic_launcher"
        android:label="@string/module_article_app_name"
        android:roundIcon="@mipmap/component_res_ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/component_res_AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    </application>

</manifest>